          "pluginVersion": "4.10.1",
          "queryType": "table",
          "rawSql": "-- SELECT\n\n-- x.value AS x_value,\n\n-- y.value AS y_value\n\n-- FROM\n\n-- (SELECT sample_index, value\n\n-- FROM measurement_data\n\n-- WHERE experiment_id = '${experiment_id}'\n\n-- AND device_name = '${device_name_x}'\n\n-- AND channel_name = '${measurement_name_x}'\n\n-- AND channel_index = ${measurement_slice}) x\n\n-- JOIN\n\n-- (SELECT sample_index, value\n\n-- FROM measurement_data\n\n-- WHERE experiment_id = '${experiment_id}'\n\n-- AND device_name = '${device_name_y}'\n\n-- AND channel_name = '${measurement_name_y}'\n\n-- AND channel_index = ${measurement_slice}) y\n\n-- ON x.sample_index = y.sample_index\n\n-- ORDER BY x.sample_index\n\n\n\nWITH\n  (SELECT min(timestamp) FROM measurement_data WHERE experiment_id = '${experiment_id}' AND device_name = '${device_name_x}') AS min_ts\n\nSELECT\n  CASE\n    WHEN endsWith('${measurement_name_x}', ' measurement time (s)') THEN\n      (toUnixTimestamp64Milli(x.timestamp) - toUnixTimestamp64Milli(min_ts)) / 1000.0\n    ELSE x.value\n  END AS x_value,\n  y.value AS y_value\nFROM\n(\n  SELECT sample_index, value, timestamp, channel_name\n  FROM measurement_data\n  WHERE experiment_id = '${experiment_id}'\n    AND device_name = '${device_name_x}'\n    AND (\n      channel_name = replaceRegexpOne('${measurement_name_x}', ' measurement time \\\\(s\\\\)$', '')\n      OR endsWith('${measurement_name_x}', ' measurement time (s)')\n    )\n    AND channel_index = ${measurement_slice}\n) x\nJOIN\n(\n  SELECT sample_index, value\n  FROM measurement_data\n  WHERE experiment_id = '${experiment_id}'\n    AND device_name = '${device_name_y}'\n    AND channel_name = '${measurement_name_y}'\n    AND channel_index = ${measurement_slice}\n) y\nON x.sample_index = y.sample_index\nORDER BY x.sample_index\n\n",
          "refId": "A"
        }
      ],
